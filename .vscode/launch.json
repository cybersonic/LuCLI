  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "üíª LucCLI Terminal",
      "request": "launch",
      "mainClass": "org.lucee.lucli.LuCLI",
      "args": ["terminal"],
      "projectName": "lucli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "java",
      "name": "üìÑ LucCLI File",
      "request": "launch",
      "mainClass": "org.lucee.lucli.LuCLI",
      "args": ["examples/hello.cfc"],
      "projectName": "lucli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "java",
      "name": "üîå LucCLI Module",
      "request": "launch",
      "mainClass": "org.lucee.lucli.LuCLI",
      "args": ["--debug", "modules", "run", "test-module"],
      "projectName": "lucli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "java",
      "name": "üîç LucCLI Help",
      "request": "launch",
      "mainClass": "org.lucee.lucli.LuCLI",
      "args": ["--help"],
      "projectName": "lucli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "java",
      "name": "‚ÑπÔ∏è LucCLI Version",
      "request": "launch",
      "mainClass": "org.lucee.lucli.LuCLI",
      "args": ["--version"],
      "projectName": "lucli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
