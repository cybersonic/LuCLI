{
  "build": {
    "prettyUrls": true,
    "defaultLayout": "page",
    "includeDrafts": false
  },
  "collections": {
    "posts": {
      "permalink": "/posts/:slug/",
      "path": "content/posts",
      "layout": "post"
    }
  },
  "navigation": {
    "rootPath": "docs"
  },
  "name": "LuCLI",
  "baseUrl": "http://localhost:8080",
  "paths": {
    "content": "content",
    "output": "public",
    "layouts": "content/layouts",
    "assets": "content/assets"
  }
}